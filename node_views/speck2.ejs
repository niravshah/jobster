<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="/bower_components/angular-material/angular-material.css">
    <link rel="stylesheet" type="text/css" href="/speck-css/v2-custom.css">
    <link rel="stylesheet" href="/bower_components/dropzone/dist/dropzone.css"/>
	<link href='http://fonts.googleapis.com/css?family=Montserrat:700|Open+Sans:600italic,400|Roboto:400,300' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
	
    <title>Specky</title>
    <script src="/bower_components/angular/angular.js"></script>
    <script src="/bower_components/angular-material/angular-material.js"></script>
</head>

<body ng-app="Speck" layout="row" ng-controller="MasterCtrl">

    <md-sidenav class="md-sidenav-left md-whiteframe-z2" md-is-open="mdIsOpen" md-component-id="left">
        <md-toolbar layout="row" class="md-whiteframe-z2">
            <div class="md-toolbar-tools">
                <md-button ng-click="toggleSideBar()" class="md-icon-button" aria-label="Close">
                    <md-icon md-svg-icon="/img/clear.svg"></md-icon>
                </md-button>
                <span flex></span>
            </div>
        </md-toolbar>
    </md-sidenav>
    <div flex layout="column">
        <md-toolbar layout="row" class="md-whiteframe-z2">
            <div class="md-toolbar-tools">
                <md-button ng-click="toggleSideBar()" class="md-icon-button" aria-label="Settings">
                    <md-icon md-svg-icon="/img/menu.svg"></md-icon>
                </md-button>
                <h2>
          <span>Specky</span>
        </h2>
                <span flex></span> 
                <md-button class="md-icon-button" aria-label="Favorite">
					<md-icon md-font-library="material-icons" class="md-light md-18">notifications</md-icon>
                </md-button>
                <md-button class="md-icon-button" aria-label="Favorite">
					<md-icon md-font-library="material-icons" class="md-light md-18">email</md-icon>
                </md-button>
            </div>
        </md-toolbar>
        <div flex layout="row">
            <div flex ui-view="main"></div>
        </div>
    </div>
	<script type="text/javascript" src="/bower_components/jquery/dist/jquery.js"></script>
    <script type="text/javascript" src="/bower_components/angular-cookies/angular-cookies.min.js"></script>
    <script type="text/javascript" src="/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script type="text/javascript" src="/bower_components/angular-sanitize/angular-sanitize.min.js"></script>
    <script type="text/javascript" src="/bower_components/angular-jwt/dist/angular-jwt.min.js"></script>
	
    <script src="/bower_components/angular-aria/angular-aria.js"></script>
    <script src="/bower_components/angular-animate/angular-animate.js"></script>
	
	<script src="http://maps.google.com/maps/api/js?sensor=true"></script>
	<script type="text/javascript" src="/bower_components/gmaps/gmaps.min.js"></script>
    
	<script type="text/javascript" src="/bower_components/dropzone/dist/dropzone.js"></script>
    
    <script type="text/javascript" src="/speck-js/module.js"></script>
    <script type="text/javascript" src="/speck-js/routes.js"></script>
    <script type="text/javascript" src="/speck-js/controllers/master-ctrl.js"></script>
    <script type="text/javascript" src="/speck-js/controllers/specs-ctrl.js"></script>
	<script type="text/javascript" src="/speck-js/controllers/v2-spec-add.js"></script>
	<script type="text/javascript" src="/speck-js/controllers/v2-spec-add-loc.js"></script>
</body>

</html>